	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:orientation="horizontal"
		android:background="@android:drawable/bottom_bar">
		<Button android:text="Back" android:id="@+id/ButtonCancelTag"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:scaleType="centerInside" android:layout_weight="1"
			android:layout_gravity="center" android:drawableLeft="@drawable/cancel"></Button>
		<Button android:text="Save" android:id="@+id/ButtonSaveTag"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:scaleType="centerInside" android:layout_weight="1"
			android:layout_gravity="center" android:drawableLeft="@drawable/save"></Button>
		<Button android:text="Upload" android:id="@+id/ButtonUpload"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:scaleType="centerInside" android:layout_weight="1"
			android:layout_gravity="center" android:drawableLeft="@drawable/upload"></Button>
	</LinearLayout>
</LinearLayout>
