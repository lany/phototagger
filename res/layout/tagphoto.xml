						android:layout_marginLeft="7dp" android:height="90dp"></EditText>
					<ImageButton android:layout_width="wrap_content"
						android:id="@+id/btnSpkComments" android:layout_height="wrap_content"
						android:background="@drawable/speak" android:layout_gravity="center_vertical"
						android:layout_marginLeft="7dp" android:layout_marginRight="13dp"></ImageButton>
				</LinearLayout>
				<LinearLayout android:layout_height="wrap_content"
					android:layout_marginTop="7dp" android:layout_marginBottom="7dp"
